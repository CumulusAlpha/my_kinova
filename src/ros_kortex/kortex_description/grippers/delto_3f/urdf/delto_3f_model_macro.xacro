<xacro:macro name="delto_3f_base_link" params="prefix parent">
        
    <joint name="${prefix}delto_3f_base_link" type="fixed">
        <parent link="${parent}"/>
        <child link="${prefix}delto_base_link"/>
        <origin rpy="0.0 0.0 1.57" xyz="0.0 0.0 0.1"/>
    </joint>

    <link name="${prefix}delto_base_link">
         <inertial>
            <origin xyz="0.00045499721796859983 -2.051648304165415e-06 -0.05218562175888181" rpy="5.671320207730568e-06 0.0032948722942074404 -0.00044870150924543735"/>
            <mass value="0.370"/>
            <inertia ixx="0.0009935903471030175" iyy="0.000980251548061173" izz="0.0003178199035240808" ixy="0" ixz="0" iyz="0"/>
         </inertial>

         <visual>
            <origin xyz="0.00045499721796859983 -2.051648304165415e-06 -0.05218562175888181" rpy="5.671320207730568e-06 0.0032948722942074404 -0.00044870150924543735"/>
            <geometry>
               <mesh filename="package://kortex_description/grippers/delto_3f/meshes/delto_base_link.stl"/>
            </geometry>
            <material name="black"/>
         </visual>

         <collision>
            <origin xyz="0.00045499721796859983 -2.051648304165415e-06 -0.05218562175888181" rpy="5.671320207730568e-06 0.0032948722942074404 -0.00044870150924543735"/>
            <geometry>
               <mesh filename="package://kortex_description/grippers/delto_3f/meshes/delto_base_link.stl"/>
            </geometry>
         </collision>
    </link>
   </xacro:macro>

