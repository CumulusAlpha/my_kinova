<xacro:macro name="delto_3f" params="parent prefix">
    <!-- Tool frame used by the arm -->
    <link name="${prefix}tool_frame"/>
    <joint name="${prefix}tool_frame_joint" type="fixed">
        <origin xyz="0 0 0.20" rpy="0 0 0" />
        <parent link="${parent}" />
        <child link="${prefix}tool_frame" />
        <axis xyz="0 0 0" />
    </joint>
    <!-- Gripper description -->

    <xacro:delto_3f_base_link prefix="${prefix}" parent="${parent}"/> -->
    <xacro:finger_links prefix="${prefix}"/>
    <xacro:finger_joints prefix="${prefix}"/>
    </xacro:macro>
</robot>